{
    "name": "passchn/cakephp-file-pool",
    "description": "FilePool plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "cakephp/cakephp": "^4.4",
        "passchn/cakephp-assets": "^0.3",
        "passchn/cakephp-vite": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "FilePool\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FilePool\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
